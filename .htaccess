RedirectMatch 301 ^/tags/([^/]+)/$ /thema/$1/
RedirectMatch 301 ^/tags/([^/]+)$ /thema/$1/
RewriteRule ^tags/([^/]+)/page/[0-9]+/?$ /thema/$1/ [R=301,L]

